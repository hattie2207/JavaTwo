
public class Converter {

	String message = ""; 

	public void digitOne(int digits) {
		
		if (digits == 0) message += "Zero";

		switch (digits) {

		case 1: 
			message += "One";
			break;
		case 2:
			message += "Two";
			break;
		case 3: 
			message += "Three";
			break;
		case 4: 
			message += "Four";
			break;
		case 5: 
			message += "Five";
			break;
		case 6: 
			message += "Six";
			break;
		case 7: 
			message += "Seven";
			break;
		case 8: 
			message += "Eight";
			break;
		case 9: 
			message += "Nine";
			break;
		case 10:
			message += "Ten";
			break;
		case 11:
			message += "Eleven";
			break;
		case 12:
			message += "Twelve";
			break;
		case 13:
			message += "Thirteen";
			break;
		case 14:
			message += "Ten";
			break;
		case 15:
			message += "Ten";
			break;
		case 16:
			message += "Ten";
			break;
		case 17:
			message += "Ten";
			break;
		case 18:
			message += "Ten";
			break;
		case 19:
			message += "Ten";
			break;


		}

	}
	
