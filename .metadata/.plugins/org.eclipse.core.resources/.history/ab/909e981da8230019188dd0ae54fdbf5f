
public class Converter {
	
	public String DigitsToWords(int digits) {
		
		String message = ""; 
		String digitsWord = "";
		
		if (digits == 0) {
			message += "Zero";
		}
		
		digitsWord = Integer.toString(digits);
		
		for (int i = 0; i < digitsWord.length(); i++){
			
			char c = digitsWord.charAt(i);

			switch (c) {
			
			case '1': 
				message += "One";
				break;
			case '2': 
				message += "One";
				break;
			case '3': 
				message += "One";
				break;
			case '4': 
				message += "One";
				break;
			case '5': 
				message += "One";
				break;
			case '6': 
				message += "One";
				break;
			case '7': 
				message += "One";
				break;
			case '8': 
				message += "One";
				break;
			case '9': 
				message += "One";
				break;
				
			}
			
//			if (digitsWord.contains("1")) {
//				message += "One";
//			}
//			else if (digitsWord.contains("2")) {
//				message += "Two";
//			}
//			else if (digitsWord.contains("3")) {
//				message += "Three";
//			}
//			else if (digitsWord.contains("4")) {
//				message += "Four";
//			}
//			else if (digitsWord.contains("5")) {
//				message += "Five";
//			}
//			else if (digitsWord.contains("6")) {
//				message += "Six";
//			}
//			else if (digitsWord.contains("7")) {
//				message += "Seven";
//			}
//			else if (digitsWord.contains("8")) {
//				message += "Eight";
//			}
//			else if (digitsWord.contains("9")) {
//				message += "Nine";
//			}
//			else {
//				
//			}	
			
			
		}			
		
		return message; 
		
	}
	
	public static void main(String[] args) {
		
		Converter c = new Converter();
		String outputMessage = c.DigitsToWords(11);
		
		System.out.println(outputMessage);
	}

}
