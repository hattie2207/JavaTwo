package calculatorThree;

import java.awt.*;

public class Calculator extends Frame {
	
	TextField t1,t2,t3;
	Label l1,l2,l3,l4;
	Button b1;
	GridLayout g1;
	
	public Calculator() {
		
		t1 = new TextField();
		t2 = new TextField();
		t3 = new TextField();
		l1 = new Label("First Number :");
		l2 = new Label("Second Number :");
		l3 = new Label();
		l4 = new Label("Result :");
		b1 = new Button();
		g1 = new GridLayout(4,2)
		
	}

}
