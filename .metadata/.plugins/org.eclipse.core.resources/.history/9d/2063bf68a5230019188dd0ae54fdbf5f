
public class Converter {
	
	public String DigitsToWords(int digits) {
		
		String message = ""; 
		if (digits == 0) {
			message = "Zero";
		}
		
		
		return ""; 
		
	}
	
	public static void main(String[] args) {
		
		Converter c = new Converter();
		String outputMessage = c.DigitsToWords(23);
	}

}
